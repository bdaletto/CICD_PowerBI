table 'List Achievements'
	lineageTag: de18a783-30e8-4324-97f8-73ffb07d431a

	measure 'Cumul Achievement temp' =
			CALCULATE (
			    SUM ( 'List Achievements'[Attribute:closed] ),
			    FILTER (
			        ALL ('List Achievements'[DateUnlock]),
			        'List Achievements'[DateUnlock] <= MAX('List Achievements'[DateUnlock]
			         )
			    )
			)
		formatString: 0
		lineageTag: b247fc67-2263-435a-b86a-a1355efed556

	measure 'Last Achievement Unlocked ID' =
			
			VAR dateMax = [Last Achievement Unlock Max Date]
			RETURN
			CALCULATE([Nom Achievement],FILTER('List Achievements','List Achievements'[DateUnlock] =dateMax))
		lineageTag: 3987378e-a4dc-45f6-a0cc-83def5d93ed5

		annotation PBI_FormatHint = {"isText":true}

	measure 'Last Achievement Unlock Max Date' = MAX('List Achievements'[DateUnlock])
		formatString: General Date
		lineageTag: efb2e7c6-20e6-4bc0-a3b9-a77281ec5f5a

	measure 'Nom Achievement' = MAX('List Achievements'[apiname])
		lineageTag: 17f48cf7-fc23-4c65-9d08-2c2db242ba28

		annotation PBI_FormatHint = {"isText":true}

	measure 'Most obtained achievement rate' = MAX('Ref Achievements'[percent])
