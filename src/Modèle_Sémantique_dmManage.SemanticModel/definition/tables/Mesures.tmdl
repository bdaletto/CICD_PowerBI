table Mesures
	lineageTag: ac27f767-9816-4e0b-b49b-295026a86f57

	measure Revenu = SUMX(F_Productions, F_Productions[Durée]*F_Productions[TJM Reel])
		formatString: #,0\ "€";-#,0\ "€";#,0\ "€"
		lineageTag: ab01974a-50e2-46e1-b484-64ad0f4d4db4

	measure DeltaTJ = SUMX(F_Productions, F_Productions[TJM Cible]-F_Productions[TJM Reel])
		formatString: #,0\ "€";-#,0\ "€";#,0\ "€"
		lineageTag: f508866b-a9ed-41c3-922d-51dca426cb22

	measure TJM =
			
			VAR Ponderee = SUMX(F_Productions, F_Productions[TJM Reel]*F_Productions[Durée])
			VAR DureeTotale = SUM(F_Productions[Durée])
			    RETURN
			          DIVIDE(Ponderee,DureeTotale,0)
		formatString: #,0\ "€";-#,0\ "€";#,0\ "€"
		lineageTag: 06ab1599-8ca6-4239-8d62-30e14e44ded7

